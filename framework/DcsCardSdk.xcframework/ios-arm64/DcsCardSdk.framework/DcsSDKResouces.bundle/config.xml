<?xml version='1.0' encoding='utf-8'?>
<widget id="io.dcs.applycard" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="CDVWebViewEngine">
        <param name="ios-package" value="CDVWebViewEngine" />
    </feature>
    <feature name="LaunchScreen">
        <param name="ios-package" value="CDVLaunchScreen" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Fingerprint">
        <param name="ios-package" value="Fingerprint" />
    </feature>
    <feature name="CordovaHttpPlugin">
        <param name="ios-package" value="CordovaHttpPlugin" />
    </feature>
    <feature name="CorHttpd">
        <param name="ios-package" value="CorHttpd" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVWKInAppBrowser" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="ionic://*" />
    <feature name="IonicWebView">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <feature name="JumioMobileSDK">
        <param name="ios-package" value="JumioMobileSDK" />
    </feature>
    <feature name="QRScanner">
        <param name="ios-package" value="QRScanner" />
    </feature>
    <feature name="DcsCardBase">
        <param name="ios-package" value="DcsCardBase" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="CameraPreview">
        <param name="ios-package" value="CameraPreview" />
    </feature>
    <name>Dcs ApplyCard SDK</name>
    <description>DcsApplyCardSdk</description>
    <author email="qiang.wang@ezynet.sg" href="https://dcscc.com">
        DCS CARD Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="https://sitcardsapp.dcscc.com/*" />
    <allow-navigation href="https://sitcardsapp.dcscc.com/*" />
    <allow-navigation href="http://localhost/*" />
    <allow-navigation href="httpsionic://*" />
    <access origin="https://sitcardsapp.dcscc.com" subdomains="true" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="MediaTypesRequiringUserActionForPlayback" value="none" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="CameraUsesGeolocation" value="false" />
    <preference name="deployment-target" value="11.0" />
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="localhost" value="app" />
    <preference name="ResolveServiceWorkerRequests" value="true" />
    <preference name="MixedContentMode" value="0" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="AndroidInsecureFileModeEnabled" value="true" />
    <preference name="AndroidBlacklistSecureSocketProtocols" value="SSLv3,TLSv1" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="ScrollEnabled" value="true" />
    <preference name="Fullscreen" value="false" />
    <preference name="iosPersistentFileLocation" value="Compatibility" />
    <preference name="iosScheme" value="httpsionic" />
</widget>
